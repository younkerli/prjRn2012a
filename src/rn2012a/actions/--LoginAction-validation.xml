<!DOCTYPE validators PUBLIC
        "-//Apache Struts//XWork Validator 1.0.2//EN"
        "http://struts.apache.org/dtds/xwork-validator-1.0.2.dtd">

<validators>
    <field name="username">
        <field-validator type="string">
        	<param name="pattern">rcgdgs</param>
            <message key="error.string" />
        </field-validator>
    </field>
    <field name="password">
        <field-validator type="string">
        	<param name="pattern">123123</param>
            <message key="error.string"/>
        </field-validator>
    </field>
</validators>
